<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.indicaleads" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>indica</name>
    <description>
        App extenção do sistema de leads www.indica.inaweb.com.br
    </description>
    <author email="vinicius@inaweb.com.br" href="http://inaweb.com.br">
        Inaweb Digital
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="Fullscreen" value="false" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="FadeSplashScreenDuration" value="1000" />
</widget>
